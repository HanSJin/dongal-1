<p id="notice"><%= notice %></p>

<h1>Listing G4 Dongal Board Studies</h1>

<table>
  <thead>
    <tr>
      <th>Wr title</th>
      <th>Wr link</th>
      <th>Wr writer</th>
      <th>Wr created on</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @g4_dongal_board_studies.each do |g4_dongal_board_study| %>
      <tr>
        <td><%= g4_dongal_board_study.wr_title %></td>
        <td><%= g4_dongal_board_study.wr_link %></td>
        <td><%= g4_dongal_board_study.wr_writer %></td>
        <td><%= g4_dongal_board_study.wr_created_on %></td>
        <td><%= link_to 'Show', g4_dongal_board_study %></td>
        <td><%= link_to 'Edit', edit_g4_dongal_board_study_path(g4_dongal_board_study) %></td>
        <td><%= link_to 'Destroy', g4_dongal_board_study, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

